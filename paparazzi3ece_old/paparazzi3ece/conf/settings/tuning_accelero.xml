

<!-- A conf to use to tune a new A/C -->

<settings>
  <dl_settings>
    <dl_settings NAME="flight params">
      <dl_setting MAX="1000" MIN="0" STEP="10" VAR="flight_altitude" shortname="altitude"/>
      <dl_setting MAX="1" MIN="0" STEP="1" VAR="light_mode"/>
      <dl_setting MAX="10" MIN="-10" STEP="0.5" VAR="wind_east"/>
      <dl_setting MAX="10" MIN="-10" STEP="0.5" VAR="wind_north"/>
    </dl_settings>
    <dl_settings NAME="mode">
      <dl_setting MAX="2" MIN="0" STEP="1" VAR="pprz_mode"/>
<!--        <dl_setting MAX="1" MIN="0" STEP="1" VAR="alt_kalman_enabled" shortname="alt_kalman"/> -->
      <dl_setting MAX="0" MIN="0" STEP="1" VAR="estimator_flight_time" shortname="flight time"/>
      <dl_setting MAX="1" MIN="0" STEP="1" VAR="launch"/>
      <dl_setting MAX="1" MIN="0" STEP="1" VAR="kill_throttle">
        <strip_button name="KILL" value="1"/>
      </dl_setting>
    </dl_settings>
    <dl_settings NAME="control">

<!--
  <dl_settings NAME="ir">
        <dl_setting MAX="1.0" MIN="-1.0" STEP="0.05" VAR="ir_roll_neutral" shortname="roll_neutral"/>
        <dl_setting MAX="0.1" MIN="-2.0" STEP="0.05" VAR="ir_pitch_neutral" shortname="pitch_neutral"/>
        <dl_setting MAX="1.5" MIN="0.5" STEP="0.1" VAR="ir_correction_left" shortname="corr_left"/>
        <dl_setting MAX="1.5" MIN="0.5" STEP="0.1" VAR="ir_correction_right" shortname="corr_right"/>
        <dl_setting MAX="1.5" MIN="0.5" STEP="0.1" VAR="ir_correction_up" shortname="corr_up"/>
        <dl_setting MAX="1.5" MIN="0.5" STEP="0.1" VAR="ir_correction_down" shortname="corr_down"/>

	<dl_setting MAX="1" MIN="0" STEP="1" VAR="ir_estim_mode"/>
</dl_settings>
-->

      <dl_settings NAME="gyro">
        <dl_setting MAX="6.0" MIN="0.0" STEP="0.05" VAR="gyro_roll_scale" shortname="roll_scale"/>
        <dl_setting MAX="6.0" MIN="0.0" STEP="0.05" VAR="gyro_pitch_scale" shortname="pitch_scale"/>
        <dl_setting MAX="6.0" MIN="0.0" STEP="0.05" VAR="gyro_yaw_scale" shortname="yaw_scale"/>
        <dl_setting MAX="512" MIN="-512" STEP="1" VAR="gyro_roll_adc" shortname="gyro_roll_adc"/>
        <dl_setting MAX="512" MIN="-512" STEP="1" VAR="gyro_pitch_adc" shortname="gyro_pitch_adc"/>
        <dl_setting MAX="512" MIN="-512" STEP="1" VAR="gyro_yaw_adc" shortname="gyro_yaw_adc"/>
      </dl_settings>
<!--
    <dl_settings NAME="range">
        <dl_setting MAX="5.0" MIN="0.0" STEP="0.05" VAR="range_scale" shortname="scale"/>
        <dl_setting MAX="1024" MIN="0" STEP="1" VAR="range_neutral" shortname="neutral"/>
      </dl_settings>
-->
        <dl_settings NAME="accelerometer">
        <dl_setting MAX="0.1" MIN="0.0" STEP="0.005" VAR="accel_ax_scale" shortname="ax_scale"/>
        <dl_setting MAX="0.1" MIN="0.0" STEP="0.005" VAR="accel_ay_scale" shortname="ay_scale"/>
        <dl_setting MAX="0.1" MIN="0.0" STEP="0.005" VAR="accel_az_scale" shortname="az_scale"/>
        <dl_setting MAX="1024" MIN="0" STEP="1" VAR="accel_ax_neutral" shortname="ax_neutral"/>
        <dl_setting MAX="1024" MIN="0" STEP="1" VAR="accel_ay_neutral" shortname="ay_neutral"/>
        <dl_setting MAX="1024" MIN="0" STEP="1" VAR="accel_az_neutral" shortname="az_neutral"/>
      </dl_settings>

      <dl_settings NAME="kalman filter">
        <dl_setting MAX="1" MIN="0.0" STEP="0.001" VAR="estimator_kalman_q_angle" shortname="Q-ANGLE"/>
        <dl_setting MAX="1" MIN="0.0" STEP="0.001" VAR="estimator_kalman_q_gyro" shortname="Q-GYRO"/>
        <dl_setting MAX="1" MIN="0.0" STEP="0.001" VAR="estimator_kalman_r_angle" shortname="R-ANGLE"/>
      </dl_settings>

      <dl_settings NAME="attitude">
<!--        <dl_setting MAX="25000" MIN="0" STEP="250" VAR="h_ctl_roll_pgain" shortname="roll_pgain"/> -->
<!--        <dl_setting MAX="25000" MIN="0" STEP="250" VAR="h_ctl_pitch_pgain" shortname="pitch_pgain"/> -->
<!--        <dl_setting MAX="2" MIN="0." STEP="0.1" VAR="h_ctl_pitch_dgain" shortname="pitch_dgain"/>  -->
<!--        <dl_setting MAX="5000" MIN="0" STEP="100" VAR="h_ctl_elevator_of_roll" shortname="elevator_of_roll"/> -->
<!--        <dl_setting MAX="5000" MIN="0" STEP="100" VAR="h_ctl_aileron_of_throttle" shortname="aileron_of_throttle"/>  -->
        <dl_setting MAX="10" MIN="-10" STEP="0.1" VAR="h_ctl_roll_rate_setpoint_pgain" shortname="rollrate_pgain"/>
        <dl_setting MAX="60000" MIN="-20000" STEP="100" VAR="h_ctl_hi_throttle_roll_rate_pgain" shortname="hi_throt_pgain"/>
        <dl_setting MAX="60000" MIN="-20000" STEP="100" VAR="h_ctl_lo_throttle_roll_rate_pgain" shortname="lo_throt_pgain"/>
        <dl_setting MAX="10" MIN="-10" STEP="0.1" VAR="h_ctl_pitch_rate_setpoint_pgain" shortname="pitchrate_pgain"/>
        <dl_setting MAX="20000" MIN="-20000" STEP="100" VAR="h_ctl_hi_throttle_pitch_rate_pgain" shortname="pitch_hi_throt_pgain"/>
        <dl_setting MAX="20000" MIN="-20000" STEP="100" VAR="h_ctl_lo_throttle_pitch_rate_pgain" shortname="pitch_lo_throt_pgain"/>
        <dl_setting MAX="10" MIN="-10" STEP="0.1" VAR="h_ctl_yaw_rate_setpoint_pgain" shortname="yawrate_pgain"/>
        <dl_setting MAX="40000" MIN="-20000" STEP="100" VAR="h_ctl_hi_throttle_yaw_rate_pgain" shortname="yaw_hi_throt_pgain"/>
        <dl_setting MAX="40000" MIN="-20000" STEP="100" VAR="h_ctl_lo_throttle_yaw_rate_pgain" shortname="yaw_lo_throt_pgain"/>

      </dl_settings>

      <dl_settings name="alt">
        <dl_setting MIN="0" MAX="1" STEP="0.01" VAR="v_ctl_altitude_pgain" shortname="alt_pgain"/>
      </dl_settings>

      <dl_settings name="auto_throttle">
	<dl_setting MAX="0.7" MIN="0.3" STEP="0.01" VAR="v_ctl_auto_throttle_cruise_throttle" shortname="cruise throttle">
        	<strip_button name="Dash" value="0.7"/>
        	<strip_button name="Loiter" value="0.52"/>
        	<strip_button name="Cruise" value="0.55"/>
	</dl_setting>
        <dl_setting MAX="0.01" MIN="0" STEP="0.0001" VAR="v_ctl_auto_throttle_cruise_throttle_pgain" shortname="cruise_throttle_pgain"/>
	<dl_setting MAX="1" MIN="0" STEP="0.001" VAR="v_ctl_auto_throttle_pgain" shortname="throttle_pgain"/>
	<dl_setting MAX="1" MIN="0" STEP="0.001" VAR="v_ctl_auto_throttle_igain" shortname="throttle_igain"/>
        <dl_setting MAX="1" MIN="0" STEP="0.001" VAR="v_ctl_auto_throttle_climb_throttle_increment" shortname="throttle_incr"/>
	<dl_setting MAX="100" MIN="0" STEP="1" VAR="vdrop2throt_coeff" shortname="throttle/1V drop"/>
        <dl_setting MAX="1" MIN="0" STEP="0.01" VAR="v_ctl_throttle_slew_" shortname="throttle_slew"/>
        <dl_setting MAX="0.01" MIN="0" STEP="0.0001" VAR="climb_throttle" shortname="climb throttle"/>
        <dl_setting MAX="0.01" MIN="0" STEP="0.0001" VAR="desc_throttle" shortname="descent throttle"/>

      </dl_settings>

      <dl_settings name="auto_pitch">
        <dl_setting MAX="1" MIN="-1" STEP="0.01" VAR="v_ctl_auto_pitch_pgain" shortname="pgain"/>
        <dl_setting MAX="1" MIN="-1" STEP="0.01" VAR="v_ctl_auto_pitch_igain" shortname="igain"/>
      </dl_settings>

      <dl_settings name="nav">
        <dl_setting MAX="1" MIN="0" STEP="0.05" VAR="h_ctl_course_pgain" shortname="course pgain"/>
        <dl_setting MAX="2" MIN="0.1" STEP="0.05" VAR="h_ctl_course_pre_bank_correction" shortname="pre bank cor"/>
        <dl_setting MAX="1" MIN="0.0" STEP="0.05" VAR="nav_glide_pitch_trim" shortname="glide pitch trim"/>
      </dl_settings>
    </dl_settings>
  </dl_settings>
</settings>
