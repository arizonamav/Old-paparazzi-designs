<flight_plan alt="220" ground_alt="170" lat0="43.5389" lon0=" 1.2433" max_dist_from_home="700" name="MAV07" qfu="180" security_height="2" wind_dir="60" wind_mod="3">
  <header>
#include "bomb.h"
#include "nav_line.h"
</header>
  <waypoints>
    <waypoint alt="ground_alt+40" name="HOME" x="753.859003724" y="-51.1591526465"/>
    <waypoint alt="ground_alt+15" name="arch_end" x="179.046764883" y="-48.7412322229"/>
    <waypoint alt="ground_alt+3" name="arch1" x="341.911346778" y="115.007166854"/>
    <waypoint alt="ground_alt+3" name="arch2" x="250.199286528" y="24.3853619993"/>
